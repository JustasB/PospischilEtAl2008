# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation 

target: "test_FS_ModelDB.hoc"
engine: NEURON
mep: ../NeuroML2/cells/FS/.test.FS.spikes.mep
experiments:
  spikes:
    observables:
      spike times:
        file: 
          path: FS.v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: 0
        tolerance: 0
