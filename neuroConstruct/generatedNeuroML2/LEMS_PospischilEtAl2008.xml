<Lems xmlns="http://www.neuroml.org/lems/0.7.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.neuroml.org/lems/0.7.2  https://raw.github.com/LEMS/LEMS/development/Schemas/LEMS/LEMS_v0.7.2.xsd">

    <Target component="sim1" reportFile="simulator.props" timesFile="time.dat"/>

        <!--Include standard NeuroML 2 ComponentType definitions-->

    <Include file="Cells.xml"/>
    <Include file="Networks.xml"/>
    <Include file="Simulation.xml"/>

    

        <!--Include the generated NeuroML 2 files-->


        <!--   Channel/synapse files-->
    <Include file="Kd_CML.channel.nml"/>
    <Include file="Km_CML.channel.nml"/>
    <Include file="LeakConductance.channel.nml"/>
    <Include file="Na_CML.channel.nml"/>
        <!--   Cell files-->
    <Include file="RS_ModelDB.cell.nml"/>
        <!--   Network file-->
    <Include file="PospischilEtAl2008.net.nml"/>
    

        <!--Note: this could be: Simulation id="sim1" ... , but Component type="Simulation" ... 
        is used to allow validation of this file according to the LEMS schema specified above...-->
    <Component type="Simulation" id="sim1" length="1000.0ms" step="0.005ms" target="network_PospischilEtAl2008">
        
        <Display id="CG_RS_ModelDB_v" title="PospischilEtAl2008: Default Simulation Configuration, CG_RS_ModelDB, VOLTAGE" xmin="-0.1" xmax="1.1" ymin="-0.104" ymax="0.064" timeScale="1s">
            <Line id="VOLTAGE" timeScale="1s" quantity="CG_RS_ModelDB/0/RS_ModelDB/v" scale="1 V" color="#000000"/>
        </Display>
    
        <OutputFile id="CG_RS_ModelDB_v_OF" fileName="CG_RS_ModelDB_0.dat">
            <OutputColumn id="v" quantity="CG_RS_ModelDB/0/RS_ModelDB/v"/>
        </OutputFile>
    </Component>


</Lems>
